@model IEnumerable<WorkSchedule.Models.DictDaysOff>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<p>
    @Html.ActionLink("Create a new", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DictDepartment.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateDayOff)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DictDepartment.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateDayOff)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.DictDaysOffId }) |
                @Html.ActionLink("Details", "Details", new { id = item.DictDaysOffId })
            </td>
        </tr>
    }

</table>

@Html.Raw(@ViewBag.Message)